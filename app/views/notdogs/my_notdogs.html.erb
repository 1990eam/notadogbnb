<div class="container">
   <% @notdogs.each do | notdog | %>
  <div class="card-notdog mb-3">
      <div class="card-notdog-infos">
        <img class="card-notdog-img", src=<%= cl_image_path notdog.photo.key, height: 400, width: 500, crop: :fill %> >

        <h1> <%= notdog.name %> </h1>
        <p> <%= notdog.description %></p>
      </div>
  </div>

  <% end %>
</div>

